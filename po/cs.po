msgstr[0] "Minulý týden"
msgstr[1] "Před {0:d} týdny"
msgstr[2] "Před {0:d} týdny"

#: ../src/gnome_abrt/tools.py:72
msgid "Last month"
msgstr "Minulý měsíc"

#. Translators: This message will never be used for less than
